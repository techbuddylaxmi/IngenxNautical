<core:FragmentDefinition xmlns="sap.m" xmlns:form="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns:l="sap.ui.layout">
    <Dialog id="_IDDialog"  title="Details" class="sapUiSmallMargin" contentHeight="450px" contentWidth="45%">

 <Panel id="ui1">
       <FlexBox  class="flex" >
    <VBox class="flex" alignItems="End" >
    
<FlexBox class="headerItem">
     <Label class="sapUiTinyMarginTop" 
     text="Vendor : "/> 
  <Input  value="{ChartingFilterModel>/vendorId}" width="100px" editable="false" />  
    </FlexBox>
    <FlexBox   class="headerItem">
     <Label  class="sapUiTinyMarginTop" text="Eligibility : "/>
      <Input   value="{ChartingFilterModel>/eligible}" width="100px" editable="false"/>  

     </FlexBox>
    <FlexBox class="headerItem">
      <Label  class="sapUiTinyMarginTop" text="Technical Ranking : "/>
  <Input   value="{ChartingFilterModel>/Trank}" width="100px" editable="false"/>  
     </FlexBox>
    <FlexBox class="headerItem">
     <Label class="sapUiTinyMarginTop" text="Class Of Vessel :"/>
      <Input  value="{ChartingFilterModel>/bidDetails/0/Value}" width="100px" editable="false"/>  

     </FlexBox>
    
      <FlexBox class="headerItem">
     <Label class="sapUiTinyMarginTop" text="Country Of Origin :"/>
      <Input  value="{ChartingFilterModel>/bidDetails/1/Value}" width="100px" editable="false"/>  

     </FlexBox>
     
      <FlexBox   class="headerItem">
     <Label  class="sapUiTinyMarginTop" text="Last Port Of Call :"/>
      <Input   value="{ChartingFilterModel>/bidDetails/2/Value}" width="100px" editable="false"/>  

     </FlexBox>
    
      <FlexBox   class="headerItem">
     <Label  class="sapUiTinyMarginTop" text="Last Cleaning Date :"/>
      <Input   value="{ChartingFilterModel>/bidDetails/4/Value}" width="100px" editable="false"/>  

     </FlexBox>
     <FlexBox class="headerItem">
     <Label  class="sapUiTinyMarginTop" text="Freight : "/>
      <Input  value="{ChartingFilterModel>/bidDetails/5/Cvalue}" width="100px" editable="false"/>  

     </FlexBox>
     
     </VBox>

     <VBox  class="flex" alignItems="End" >
     <FlexBox class="headerItem" >
      <Label class="sapUiTinyMarginTop" text="  "/>
  <Input  value="{}" editable="false" width="100px" visible="false"/>  
     </FlexBox>
       <FlexBox class="headerItem">
      <Label class="sapUiTinyMarginTop" text="Score : "/>
  <Input  value="{ChartingFilterModel>/score}" width="100px" editable="false"/>  
     </FlexBox>
      <FlexBox class="headerItem">
      <Label class="sapUiTinyMarginTop" text="Commercial Ranking : "/>
  <Input  value="{ChartingFilterModel>/Crank}" width="100px" editable="false"/>  
     </FlexBox>
     
      <FlexBox   class="headerItem">
     <Label  class="sapUiTinyMarginTop" text="Score-Class Of Vessel :"/>
      <Input   value="{ChartingFilterModel>/bidDetails/0/fScore}" width="100px" editable="false"/>  

     </FlexBox>
     <FlexBox   class="headerItem">
     <Label  class="sapUiTinyMarginTop" text="Score-Country Of Origin : "/>
      <Input   value="{ChartingFilterModel>/bidDetails/1/fScore}" width="100px" editable="false"/>  

     </FlexBox>
      <FlexBox  class="headerItem">
     <Label  class="sapUiTinyMarginTop" text="Score-Last Port Of Call :"/>
      <Input   value="{ChartingFilterModel>/bidDetails/2/fScore}" width="100px" editable="false"/>  

     </FlexBox>
     <FlexBox  class="headerItem">
     <Label  class="sapUiTinyMarginTop" text="Score-Last Cleaning Date :"/>
      <Input   value="{ChartingFilterModel>/bidDetails/4/fScore}" width="100px" editable="false"/>  

     </FlexBox>
     <FlexBox  class="headerItem">
     <Label  class="sapUiTinyMarginTop" text="Demurrage :"/>
  <Input value="{ChartingFilterModel>/bidDetails/3/Cvalue}" width="100px" editable="false"/>  
     </FlexBox>
     
   
     
     
      
      
     
     </VBox>
     </FlexBox>
     
     
     
    </Panel>

    


         <footer>
        	<OverflowToolbar id="_IDGenOverflowToolbar2" >	
                <ToolbarSpacer id="_IDGenToolbarSpacer2"/>
                <Button id="_IDGenButto" text="Close" press="oncancell">
				</Button>
			</OverflowToolbar>
        </footer>
    </Dialog>
</core:FragmentDefinition>   